package tes;

import java.io.FileWriter;
import java.io.IOException;

public class aZero {
	static String emoji = "\uD83D\uDEAB";
	
	public static void main(String[] args) {
try (FileWriter file = new FileWriter("C:\\Users\\fabricio\\Desktop\\teste.txt")) {
	for(int i = 1; i <= 5; i++) {
		for(int j = 1; j <= i; j++) {
	file.write(emoji);
		}
		file.write("\n");	
	}
	file.close();
}catch(IOException e) {
	System.out.print("ocorreu um erro inesperado" );
	e.printStackTrace();
		}
	}
}
